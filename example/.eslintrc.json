{
    "parser": "babel-eslint",
    "extends": ["google", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins":[
        "react"
    ],
    "rules": {
        "max-len": "off"
    }
}
